fn main() {
    // Basic: 2 ** 10 = 1024
    let a: int = 2 ** 10;
    print(a);

    // Right-associativity: 2 ** 3 ** 2 = 2 ** (3 ** 2) = 2 ** 9 = 512
    let b: int = 2 ** 3 ** 2;
    print(b);

    // Precedence over multiplication: 2 * 3 ** 2 = 2 * (3 ** 2) = 2 * 9 = 18
    let c: int = 2 * 3 ** 2;
    print(c);

    // Power of 1 and 0
    let d: int = 5 ** 0;
    print(d);

    let e: int = 5 ** 1;
    print(e);
}
